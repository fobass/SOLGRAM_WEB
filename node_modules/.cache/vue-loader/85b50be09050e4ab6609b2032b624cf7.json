         //     iconName.value = 'error'\n            // } else {\n            //     iconName.value = ''\n            // }\n        }\n    })\n\n    const onBlur = () => {\n        if (!isFocused.value) {\n            blockClass.value = (isError.value == 0) ? 'block' : ''\n            // if (iconName.value == '') {\n            //     iconName.value = 'edit'\n            //     blockClass.value = 'block'\n            // }\n            if (isError.value != -1) {\n                isError.value = 1\n            }\n        }\n        isFocused.value = true\n    }\n\n    const onClick = () => {\n        iconName.value = ''\n        blockClass.value = ''\n        const input = inputName.value as any\n        isFocused.value = false\n        isError.value = 0\n        input.focus()\n    }\n\n</script>\n<style lang=\"scss\" scoped>\n\n.error {\n    content: url(/ic-svg/ic_user_off.svg);\n}\n.edit {\n    content: url(/ic-svg/ic_gear_off.svg);\n    opacity: 0;\n    z-index: -1;\n}\n.block input{\n    background: rgb(136, 46, 46);\n}\n.input-container {\n    position: relative;\n    width: 200px;\n}\n.input-container:hover .k-icon.edit {\n    z-index: 1;\n    opacity: 1;\n}\n.input-container .k-icon {\n    position: absolute;\n    top: 0px;\n    right: -5px;\n    width: 25px;\n    height: 24px;\n    background-color: #2af;\n    border-top-right-radius: 6px;\n    border-bottom-right-radius: 6px;\n}\n.input-container input {\n    display: inline-block;\n    width: 100%;\n    font-size: 1em;\n    height: 22px;\n    border-radius: 6px;\n    border: 0px;\n    box-shadow: 0px 0px 5px 0px rgba(120,120,120,0.5);\n}\n\n\n</style>"]}]}